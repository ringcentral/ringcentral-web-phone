  "main": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && yarn tsc && yarn rollup -c && rm -rf docs && parcel build demo/index.html demo/callback.html --dist-dir docs --public-url ./",
    "lint": "tsc --noEmit --target ESNext --moduleResolution bundler ./src/*.ts && eslint --fix '**/*.{ts,tsx,js,jsx}' && prettier --write . && sort-package-json",
    "serve": "rm -rf .parcel-cache && parcel demo/index.html demo/callback.html --dist-dir docs --no-hmr",
    "test": "jest src/mediaStreams.spec.ts",
    "test2": "NODE_OPTIONS=--require=dotenv-override-true/config playwright test test/index.spec.ts --workers=1",
    "test:coverage": "cat .coverage/lcov.info | coveralls -v",
    "deploy": "gh-pages -d docs --repo=https://github.com/ringcentral/web-phone-demo-1.git --branch=gh-pages"
  },
  "dependencies": {
    "sip.js": "^0.21.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.54.2",
    "@rc-ex/core": "^1.7.0",
    "@ringcentral/sdk": "^5.0.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "bootstrap": "3.4.1",
    "buffer": "^6.0.3",
    "coveralls": "^3.1.1",
    "crypto-browserify": "^3.12.1",
    "dotenv-override-true": "^6.2.2",
    "eslint": "8.57.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.3",
    "events": "^3.3.0",
    "fix-esm": "^1.0.1",
    "gh-pages": "^6.3.0",
    "http-server": "^14.1.1",
    "jest": "^30.0.5",
    "jquery": "^3.7.1",
    "parcel": "^2.15.4",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "rollup": "^4.46.2",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "sort-package-json": "^3.4.0",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ttpt": "^0.13.0",
    "typescript": "^5.9.2",
    "vm-browserify": "^1.1.2",
    "wait-for-async": "^0.7.13",
    "yarn-upgrade-all": "^0.7.5"
  },
  "preferGlobal": false,
  "targets": {
    "main": false,
    "module": false,
    "types": false,
    "browser": false
  },
  "yarn-upgrade-all": {
    "ignore": [
      "bootstrap",
      "eslint"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
